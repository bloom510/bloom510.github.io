{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bloom510.github.io/card",
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-swipeable-views": "^0.13.0",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "sass:watch": "node-sass -w ./src/styles/scss -o ./src/styles/css",
    "sass:build": "node-sass ./src/styles/scss -o ./src/styles/css --source-map true",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b master -d build",
    "start": "yarn run sass:build && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "setup": "yarn install && cd client && yarn install"
  },
  "proxy": "http://localhost:3090"
}
